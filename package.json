{
  "name": "Trello_UI_Testing_Cucumber",
  "type": "module",
  "devDependencies": {
    "@wdio/cli": "^9.12.7",
    "@wdio/cucumber-framework": "^9.12.6",
    "@wdio/local-runner": "^9.12.7",
    "@wdio/spec-reporter": "^9.12.6",
    "@wdio/visual-service": "^7.0.0"
  },
  "scripts": {
    "wdio": "wdio run ./wdio.conf.ts",
    "smoke": "wdio run ./wdio.conf.ts --cucumberOpts.tagExpression=@smoke",
    "positive_tests": "wdio run ./wdio.conf.ts --cucumberOpts.tagExpression=@positive",
    "negative_tests": "wdio run ./wdio.conf.ts --cucumberOpts.tagExpression=@negative"

  }
}
